{
  "name": "@thru/indexer",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "dev": "tsx watch src/index.ts",
    "test": "vitest run",
    "lint": "eslint src/"
  },
  "dependencies": {
    "@hono/zod-openapi": "0.18.4",
    "drizzle-orm": "^0.38.0",
    "drizzle-zod": "^0.8.3",
    "hono": "^4.6.0",
    "zod": "^3.23.0"
  },
  "peerDependencies": {
    "@thru/replay": "workspace:*",
    "postgres": "^3.4.0"
  },
  "devDependencies": {
    "@thru/replay": "workspace:*",
    "@types/node": "^20.11.0",
    "postgres": "^3.4.0",
    "tsup": "^8.1.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  }
}
