{
  "name": "@thru/replay",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "run:block-replay": "tsx scripts/run-block-replay.ts",
    "run:token-events": "tsx scripts/replay-token-events.ts",
    "run:token-accounts": "tsx scripts/stream-token-accounts.ts",
    "run:replays": "tsx scripts/run-replays.ts",
    "test:list-accounts": "tsx scripts/test-list-accounts.ts",
    "test:backfill": "tsx scripts/test-account-backfill.ts",
    "typecheck": "tsc --noEmit",
    "dev": "tsx watch src/index.ts",
    "test": "vitest run",
    "test:run": "vitest run",
    "lint": "eslint src/"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.9.0",
    "@connectrpc/connect": "^2.1.0",
    "@connectrpc/connect-node": "^2.1.0",
    "@thru/proto": "workspace:*",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@thru/helpers": "workspace:*",
    "@types/node": "^20.11.0",
    "tsup": "^8.1.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  }
}
